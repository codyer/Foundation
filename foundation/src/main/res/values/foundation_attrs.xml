<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="foundation_picker_view">
        <attr name="foundation_picker_view_gravity">
            <enum name="center" value="17"/>
            <enum name="left" value="3"/>
            <enum name="right" value="5"/>
        </attr>
        <attr name="foundation_picker_view_text_size" format="dimension"/>
        <attr name="foundation_picker_view_text_color_out" format="color"/>
        <attr name="foundation_picker_view_text_color_center" format="color"/>
        <attr name="foundation_picker_view_divider_color" format="color"/>
    </declare-styleable>

    <!--AvatarView-->
    <declare-styleable name="Foundation.AvatarView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="shadow_width" format="dimension" />
        <attr name="shadow_color" format="color" />
    </declare-styleable>

</resources>